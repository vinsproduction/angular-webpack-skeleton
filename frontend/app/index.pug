			body {
				margin: 0;
				background: #FFF;
			}



			body .main-preloader {
				background: rgb(57,57,57);
				visibility: visible;
				opacity: 1;
				top: 0;
				z-index: 9999;
				position: fixed;
				width: 100%;
				height: 100%;
				color: #000;
				overflow: hidden;
			}

			body .main-preloader.main-preloader-inactive {
				visibility: hidden;
				opacity: 0;
				-moz-opacity: 0;
				filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=0);
				transition: visibility 0.81s, opacity 0.8s linear;
				-o-transition: visibility 0.81s, opacity 0.8s linear;
				-ms-transition: visibility 0.81s, opacity 0.8s linear;
				-webkit-transition: visibility 0.81s, opacity 0.8s linear;
				-moz-transition: visibility 0.81s, opacity 0.8s linear;
			}

			body .main-preloader .preloader {
				position: absolute;
				top: 50%;
				left: 50%;
				margin-left: -49px;

			}


			.main-preloader .preloader > div {
			  background-color: #CCC;
			  width: 20px;
			  height: 20px;
			  border-radius: 100%;
			  margin: 0px 5px;
			  animation-fill-mode: both;
			  -o-animation-fill-mode: both;
			  -ms-animation-fill-mode: both;
			  -webkit-animation-fill-mode: both;
			  -moz-animation-fill-mode: both;
			  display: inline-block;
			}

			body .main-preloader .preloader > div:nth-child(1) {
			  animation: scale 1s -0.24s infinite;
			  -o-animation: scale 1s -0.24s infinite;
			  -ms-animation: scale 1s -0.24s infinite;
			  -webkit-animation: scale 1s -0.24s infinite;
			  -moz-animation: scale 1s -0.24s infinite;
			}
			.main-preloader .preloader > div:nth-child(2) {
			  animation: scale 1s -0.12s infinite;
			  -o-animation: scale 1s -0.12s infinite;
			  -ms-animation: scale 1s -0.12s infinite;
			  -webkit-animation: scale 1s -0.12s infinite;
			  -moz-animation: scale 1s -0.12s infinite;
			}
			body .main-preloader .preloader > div:nth-child(3) {
			  animation: scale 1s 0s infinite;
			  -o-animation: scale 1s 0s infinite;
			  -ms-animation: scale 1s 0s infinite;
			  -webkit-animation: scale 1s 0s infinite;
			  -moz-animation: scale 1s 0s infinite;
			}


